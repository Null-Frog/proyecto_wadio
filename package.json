{
  "name": "proyecto_wadio",
  "productName": "Wadio!",
  "version": "1.0.0",
  "description": "Bot de WhatsApp que transcribe un audio a texto y lo contesta con un mensaje personalizado",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder"
  },
  "build": {
    "appId": "cool-app",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "whatsapp-botsIN.ico"
    },
    "nsis": {
      "installerIcon": "whatsapp-botsIN.ico",
      "uninstallerIcon": "whatsapp-botsUN.ico",
      "uninstallDisplayName": "Wadio!",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": {
    "name": "rubentxogg",
    "email": "rubentxogg@hotmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "proyecto_wadio"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "ibm-watson": "^6.1.1",
    "qrcode": "^1.4.4",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.12.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.58",
    "@electron-forge/maker-deb": "^6.0.0-beta.58",
    "@electron-forge/maker-rpm": "^6.0.0-beta.58",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.58",
    "@electron-forge/maker-zip": "^6.0.0-beta.58",
    "electron": "13.1.7",
    "electron-builder": "^22.11.7"
  }
}
